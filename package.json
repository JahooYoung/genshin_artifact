{
  "name": "genshin_artifacts",
  "version": "5.0.2",
  "private": true,
  "scripts": {
    "serve": "cross-env MONA_TITLE=莫娜占卜铺 MONA_NEED_BEIAN=false MONA_NEED_MIGRATE=false MONA_ROUTE_MODE=hash vue-cli-service serve",
    "build": "cross-env MONA_TITLE=莫娜占卜铺 MONA_NEED_BEIAN=false MONA_NEED_MIGRATE=false MONA_ROUTE_MODE=hash vue-cli-service build",
    "build:gh": "cross-env MONA_TITLE=莫娜占卜铺·改 MONA_NEED_BEIAN=false MONA_NEED_MIGRATE=true MONA_ROUTE_MODE=hash vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:1": "cross-env MONA_TITLE=原神圣遗物工具 MONA_NEED_BEIAN=true MONA_BEIAN_NUMBER=1 MONA_NEED_MIGRATE=true MONA_ROUTE_MODE=hash vue-cli-service build",
    "build:2": "cross-env MONA_TITLE=莫娜占卜铺 MONA_NEED_BEIAN=true MONA_BEIAN_NUMBER=2 MONA_NEED_MIGRATE=false MONA_ROUTE_MODE=history vue-cli-service build",
    "download-artifact": "node ./script/download_artifact.js",
    "inspect": "vue inspect > inspect.js",
    "myserve": "serve ./dist",
    "pack": "tar -czvf dist.tar.gz ./dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "core-js": "^3.20.3",
    "element-ui": "^2.15.6",
    "fuse.js": "^6.5.3",
    "genshin_panel": "^2.21.0",
    "is-electron": "^2.2.0",
    "localforage": "^1.10.0",
    "object-hash": "^2.2.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@vue/cli-plugin-babel": "^5.0.0-rc.2",
    "@vue/cli-plugin-eslint": "^5.0.0-rc.2",
    "@vue/cli-service": "^5.0.0-rc.2",
    "axios": "^0.26.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^8.2.0",
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0",
    "node-html-parser": "^3.2.0",
    "sass": "^1.32.10",
    "sass-loader": "^10.1.1",
    "vue-router": "^3.4.8",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.5.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
